{
  "name": "vite-plugin-use-electron",
  "version": "0.2.6",
  "description": "A Vite plugin to simplify using Electron APIs in the renderer process",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brettlamy/vite-plugin-use-electron.git"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "electron",
    "renderer",
    "ipc"
  ],
  "author": "Brett Lamy",
